      "CIJHEMKHFLG": 3
    },
    {
      "LGJCPENNFBC": 100049,
      "BKCIGACKEFD": 1,
      "HBJAHCGKJJD": "闪电",
      "BDAIMBLHHEF": "累了？我想想……",
      "CIJHEMKHFLG": 3
    },
    {
      "LGJCPENNFBC": 100050,
      "BKCIGACKEFD": 1,
      "HBJAHCGKJJD": "闪电",
      "BDAIMBLHHEF": "指挥官，稍等。",
      "CIJHEMKHFLG": 3
    },
    {
      "LGJCPENNFBC": 100051,
      "BKCIGACKEFD": 4,
      "BDAIMBLHHEF": "闪电走开片刻，随后将一枚软垫递了过来。",
      "CIJHEMKHFLG": 4
    },
    {
      "LGJCPENNFBC": 100052,
      "BKCIGACKEFD": 1,
      "HBJAHCGKJJD": "闪电",
      "BDAIMBLHHEF": "靠着它会舒服一些。",
      "CIJHEMKHFLG": 3
    },
    {
      "LGJCPENNFBC": 100053,
      "BKCIGACKEFD": 3,
      "HBJAHCGKJJD": "指挥官",
      "BDAIMBLHHEF": "谢了。",
      "CIJHEMKHFLG": 2
    },
    {
      "LGJCPENNFBC": 100054,
      "BKCIGACKEFD": 4,
      "BDAIMBLHHEF": "我顺手接过软垫，将它抱在怀里。",
      "CIJHEMKHFLG": 3
    },
    {
      "LGJCPENNFBC": 100055,
      "BKCIGACKEFD": 1,
      "HBJAHCGKJJD": "闪电",
