    },
    {
      "LGJCPENNFBC": 145,
      "LMDJKJCPJHF": "今天辛苦啦！"
    },
    {
      "LGJCPENNFBC": 146,
      "LMDJKJCPJHF": "距离我们的休闲计划又更近了一步呢！"
    },
    {
      "LGJCPENNFBC": 147,
      "LMDJKJCPJHF": "昨天，辛苦你了~"
    },
    {
      "LGJCPENNFBC": 148,
      "LMDJKJCPJHF": "来帮我收拾房间~"
    },
    {
      "LGJCPENNFBC": 149,
      "LMDJKJCPJHF": "不辛苦"
    },
    {
      "LGJCPENNFBC": 250,
      "LMDJKJCPJHF": "进入战斗状态的你，会因为过于投入，无意识的受很多伤"
    },
    {
      "LGJCPENNFBC": 251,
      "LMDJKJCPJHF": "我不希望你再受到伤害了"
    },
    {
      "LGJCPENNFBC": 252,
      "LMDJKJCPJHF": "没关系的"
    },
    {
      "LGJCPENNFBC": 253,
      "LMDJKJCPJHF": "只要记忆体能保存下来，破损的零件都可以进行换修"
    },
    {
      "LGJCPENNFBC": 254,
      "LMDJKJCPJHF": "虽然时常出现记忆丢失的情况"
    },
    {
      "LGJCPENNFBC": 255,
      "LMDJKJCPJHF": "我也会通过其它的方法，保存重要的记忆"
    },
    {
      "LGJCPENNFBC": 256,
      "LMDJKJCPJHF": "指挥官"
    },
    {
      "LGJCPENNFBC": 257,
      "LMDJKJCPJHF": "来一起看看吗？"
    },
    {
      "LGJCPENNFBC": 258,
